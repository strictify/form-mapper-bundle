{
    "name": "strictify/form-mapper-bundle",
    "description": "Strictify your forms",
    "type": "symfony-bundle",
    "license": "MIT",
    "keywords": [
        "form mapper"
    ],
    "authors": [
        {
            "name": "Zeljko Mitic",
            "email": "mitke013@gmail.com"
        }
    ],
    "require": {
        "php": "^8.1",
        "ext-json": "*",
        "symfony/form": "^5.4|^6.0|^7.0",
        "symfony/framework-bundle": "^5.4|^6.0|^7.0",
        "symfony/validator": "^5.4|^6.0|^7.0"
    },
    "require-dev": {
        "roave/security-advisories": "dev-latest",
        "symfony/phpunit-bridge": "^5.0|^6.0|^7.0",
        "symfony/var-dumper": "*",
        "vimeo/psalm": "^5",
        "psalm/plugin-symfony": "^5"
    },
    "autoload": {
        "psr-4": {
            "Strictify\\FormMapper\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Strictify\\FormMapper\\Tests\\": "tests",
            "Strictify\\FormMapper\\Tests\\Fixture\\": "tests\\fixtures"
        }
    },
    "scripts": {
        "check-style": "phpcs src tests",
        "fix-style": "phpcbf src tests"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    },
    "config": {
    }
}
