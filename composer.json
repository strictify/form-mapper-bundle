{
    "name": "strictify/form-mapper-bundle",
    "description": "Strictify your forms",
    "type": "symfony-bundle",
    "license": "MIT",
    "keywords": [
        "form mapper"
    ],
    "authors": [
        {
            "name": "Zeljko Mitic",
            "email": "mitke013@gmail.com"
        }
    ],
    "require": {
        "php": "^8.1",
        "ext-json": "*",
        "doctrine/annotations": "^1.2",
        "doctrine/collections": "^1.2|^2",
        "sensio/framework-extra-bundle": "^5.4|^6.0",
        "symfony/form": "^5.4|^6.0",
        "symfony/framework-bundle": "^5.4|^6.0",
        "symfony/options-resolver": "^5.4|^6.0",
        "symfony/validator": "^5.4|^6.0",
        "symfony/yaml": "^5.4|^6.0",
        "doctrine/instantiator": "^1.3"
    },
    "require-dev": {
        "roave/security-advisories": "dev-latest",
        "symfony/browser-kit": "^4.0|^5.0",
        "symfony/css-selector": "^4.0",
        "symfony/phpunit-bridge": "^4.0",
        "symfony/var-dumper": "^4.0",
        "symplify/easy-coding-standard": "^9",
        "vimeo/psalm": "4.x-dev",
        "psalm/plugin-symfony": "dev-master"
    },
    "autoload": {
        "psr-4": {
            "Strictify\\FormMapper\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Strictify\\FormMapper\\Tests\\": "tests",
            "Strictify\\FormMapper\\Tests\\Fixture\\": "tests\\fixtures"
        }
    },
    "scripts": {
        "check-style": "phpcs src tests",
        "fix-style": "phpcbf src tests"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    },
    "config": {
    }
}
